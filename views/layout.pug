doctype html
html
  head
    title= title
    link(rel='stylesheet' href='/css/bootstrap.min.css')
    link(rel='stylesheet' href='/css/bootstrap.min.css')
    link(rel='stylesheet' href='stylesheets/style.css')
    script(src='/js/jquery.min.js')
    script(src='/js/bootstrap.min.js')
  body
    nav.navbar.navbar-expand-lg.navbar-dark.bg-dark(style='margin:24px 0;')      
      button.navbar-toggler.navbar-toggler-right(type='button' data-toggle='collapse' data-target='#navb')
        span.navbar-toggler-icon
      #navb.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
          li.nav-item
            a.nav-link(href='/') Home 
          if !logedIn 
            li.nav-item
              a.nav-link(href='/auth/google') Google +
            li.nav-item
              a.nav-link(href='/auth/facebook') Facebook
            li.nav-item
              a.nav-link(href='/auth/twitter') Twitter
            li.nav-item
              a.nav-link(href='/auth/add') Register
            li.nav-item
              a.nav-link(href='/auth/login') Login
    
          li.nav-item
            a.nav-link(href='/dashboard') Dahboard
          li.nav-item
            a.nav-link(href='/about') About

        ul.my-2.my-lg-0
          if logedIn 
            li.nav-item
              a.nav-link(href='/auth/logout') Logout
    .container
        include partials/message
        block content
          